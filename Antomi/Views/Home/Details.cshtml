
@model Product
<!--breadcrumbs area start-->
<div class="breadcrumbs_area">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb_content">
                    <ul>
                        <li><a href="index.html">home</a></li>
                        <li>product details</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!--breadcrumbs area end-->

<div class="product_page_bg">
    <div class="container">
        <div class="product_details_wrapper mb-55">
            <!--product details start-->
            <div class="product_details">
                <div class="row">
                    <div class="col-lg-5 col-md-6">
                        <div class="product-details-tab">
                            <div id="img-1" class="zoomWrapper single-zoom">
                                <a href="">
                                    <img id="zoom1" src="~/image/product/@Model.ProductColors.First().ProductColorImages.FirstOrDefault(x=>x.IsMain).Image" data-zoom-image="~/image/product/@Model.ProductColors.First().ProductColorImages.FirstOrDefault(x=>x.IsMain).Image">
                                </a>
                            </div>
                            <div class="single-zoom-thumb">
                                <ul class="s-tab-zoom owl-carousel single-product-active" id="gallery_01">
                                    @foreach (var item in Model.ProductColors.First().ProductColorImages.Where(x => !x.IsMain).ToList())
                                    {
                                        <li>
                                            <a href="#" class="elevatezoom-gallery active" data-update="" data-image="~/image/product/@item.Image" data-zoom-image="~/image/product/@item.Image">
                                                <img src="~/image/product/@item.Image" alt="zo-th-1" />
                                            </a>

                                        </li>
                                    }

                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-6">
                        <div class="product_d_right">
                            <form action="#">

                                <h3><a href="#">@Model.Model</a></h3>
                                <div class="product_rating">
                                    <ul>
                                        <li><a href="#"><i class="ion-android-star-outline"></i></a></li>
                                        <li><a href="#"><i class="ion-android-star-outline"></i></a></li>
                                        <li><a href="#"><i class="ion-android-star-outline"></i></a></li>
                                        <li><a href="#"><i class="ion-android-star-outline"></i></a></li>
                                        <li><a href="#"><i class="ion-android-star-outline"></i></a></li>
                                        <li class="review"><a href="#">(1 customer review )</a></li>
                                    </ul>
                                </div>
                                <div id="divpriceId" class="price_box">
                                    @if (Model.ProductColors.First().Discounts.FirstOrDefault(x => x.IsActive) != null)
                                    {
                                        double amount = Model.ProductColors.First().Price * (100 - Model.ProductColors.First().Discounts.FirstOrDefault(x => x.IsActive).Percent) / 100;

                                        <span id="oldpriceId" class="old_price">$@Model.ProductColors.First().Price</span>
                                        <span id="currentpriceId" class="current_price">$@Math.Round(amount, 2)</span>

                                    }
                                    else
                                    {
                                        <span id="oldpriceId" class="old_price"></span>
                                        <span id="currentpriceId" class="current_price">$@Math.Round(Model.ProductColors.First().Price, 2)</span>
                                    }
                                </div>
                                <div class="product_desc">
                                    <p>@(Model.Description.Length < 200? Model.Description:Model.Description.Substring(0, 200)+"...") </p>
                                </div>
                                <div class="product_variant color">
                                    <h3>Available Options</h3>
                                    <label>color</label>
                                    <ul>
                                        @foreach (var item in Model.ProductColors)
                                        {
                                            <li class="color1"><a style="background:@item.Name" class="btn" onclick='getData(@item.Id)'></a></li>
                                        }
                                    </ul>
                                </div>
                                <div class="product_variant quantity">
                                    <label>quantity</label>
                                    <input min="1" max="@Model.ProductColors.First().Count" value="1" type="number" id="quantityId">
                                    <button id="addtocartId" data-id="@Model.ProductColors.First().Id" onclick="AddtoBasket()" class="button" type="button">add to cart</button>

                                </div>
                                <div class=" product_d_action">
                                    <ul>
                                        <li><a onclick="AddtoWishlist()" title="Add to wishlist">+ Add to Wishlist</a></li>
                                        <li><a href="#" title="Add to Compare">+ Compare</a></li>
                                    </ul>
                                </div>
                                <div class="product_meta">
                                    <span>Category: <a href="#">@Model.SubCategory.Category.Name</a></span>
                                </div>

                            </form>
                            <div class="priduct_social">
                                <ul>
                                    <li><a class="facebook" href="#" title="facebook"><i class="fa fa-facebook"></i> Like</a></li>
                                    <li><a class="twitter" href="#" title="twitter"><i class="fa fa-twitter"></i> tweet</a></li>
                                    <li><a class="pinterest" href="#" title="pinterest"><i class="fa fa-pinterest"></i> save</a></li>
                                    <li><a class="google-plus" href="#" title="google +"><i class="fa fa-google-plus"></i> share</a></li>
                                    <li><a class="linkedin" href="#" title="linkedin"><i class="fa fa-linkedin"></i> linked</a></li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <!--product details end-->
            <!--product info start-->
            <div class="product_d_info">
