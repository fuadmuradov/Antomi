                                                            <tr>
                                                                <td class="first_child">Processor</td>
                                                                <td>@item.Processor</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">RAM</td>
                                                                <td>@item.RAM</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Operating System</td>
                                                                <td>@item.OS</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Storage</td>
                                                                <td>@item.Storage</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Display Type</td>
                                                                <td>@item.DisplayType</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Main Camera</td>
                                                                <td>@item.MainCamera</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Selfie Camera</td>
                                                                <td>@item.SelfieCamera</td>
                                                            </tr>
                                                        }

                                                    }
                                                    @if (Model.NotebookSpecifications.Count != 0)
                                                    {
                                                        foreach (var item in Model.NotebookSpecifications)
                                                        {
                                                            <tr>
                                                                <td class="first_child">Processor</td>
                                                                <td>@item.Processor</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">RAM</td>
                                                                <td>@item.RAM</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Operating System</td>
                                                                <td>@item.OS</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Storage</td>
                                                                <td>@item.Storage</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="first_child">Graphic</td>
                                                                <td>@item.Graphics</td>
                                                            </tr>

                                                        }

                                                    }
                                                    @if (Model.Specifications.Count != 0)
                                                    {
                                                        foreach (var item in Model.Specifications)
                                                        {
                                                            <tr>
                                                                <td class="first_child">@item.Name</td>
                                                                <td>@item.Description</td>
                                                            </tr>
                                                        }
                                                    }

                                                </tbody>
                                            </table>
                                        </form>
                                    </div>
                                    <div class="product_info_content">
                                        <p>@(Model.Description.Length < 300? Model.Description:Model.Description.Substring(0, 300)+"...")</p>
                                    </div>
                                </div>

                                <div class="tab-pane fade" id="reviews" role="tabpanel">
                                    <div class="reviews_wrapper">
                                        <h2>1 review for Donec eu furniture</h2>
                                        <div class="reviews_comment_box">
                                            <div class="comment_thmb">
                                                <img src="~/assets/img/blog/comment2.jpg" alt="">
                                            </div>
                                            <div class="comment_text">
                                                <div class="reviews_meta">
